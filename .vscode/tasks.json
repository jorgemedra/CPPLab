{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: Build for debug with g++",
            "command": "g++",
            "args": [
                "-g",
                "-pthread",
                "-lstdc++fs",
                "-std=c++17",
                "-MD",
                "${fileDirname}/const.cpp",
                "${fileDirname}/constex.cpp",
                "${fileDirname}/file.cpp",
                "${fileDirname}/lambdas.cpp",
                "${fileDirname}/main.cpp",
                "${fileDirname}/mutable.cpp",
                "${fileDirname}/pointer.cpp",
                "${fileDirname}/rlvalue.cpp",
                "${fileDirname}/smartptr.cpp",
                "${fileDirname}/str_view.cpp",
                "${fileDirname}/template.cpp",
                "${fileDirname}/threads.cpp",
                "${fileDirname}/timetst.cpp",
                "${fileDirname}/virtual.cpp",
                "-o",
                "${fileDirname}/../bin/tutorial"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}